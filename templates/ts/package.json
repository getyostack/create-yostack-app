{
  "name": "{{appName}}",
  "private": true,
  "scripts": {
    "start": "webpack-cli serve",
    "build": "webpack --mode production",
    "serve": "serve dist -p 5050",
    "clean": "rm -rf dist",
    "bootstrap": "lerna bootstrap",
    "update-dev": "npx yostack-update-dev",
    "public-release": "npm run build && npx yostack-release-code public",
    "custom-release": "npm run build && npx yostack-release-code custom"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-modal": "^3.14.4"
  },
  "devDependencies": {
    "@types/react-modal": "^3.13.1",
    "create-yostack-app": ">=0.0.7",
    "lerna": "^4.0.0",
    "postcss": "^8.4.5",
    "sass": "^1.45.2",
    "serve": "^13.0.2",
    "ts-loader": "^9.2.8",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  }
}
